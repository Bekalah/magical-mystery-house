{
  "name": "@cathedral/godot-codex-14499",
  "version": "1.0.0",
  "description": "Godot Codex 144:99 - Sacred lattice system for games and applications",
  "main": "codex_14499.gd",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "validate": "echo 'Validating sacred mathematics...' && node validate-sacred-math.js",
    "export": "echo 'Exporting for Godot...' && pnpm run build",
    "test": "echo 'Testing lattice connections...' && node test-connections.js",
    "dev": "tsx watch src/index.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build",
    "clean:all": "rimraf dist build node_modules/.turbo",
    "validate:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "benchmark": "vitest run --reporter=verbose --reporter=json --outputFile=benchmark-results.json",
    "validate:sacred-math": "node scripts/validate-sacred-math.js",
    "validate:trauma-safety": "node scripts/validate-trauma-safety.js",
    "validate:integration": "node scripts/validate-integration.js",
    "validate:ownership": "node scripts/validate-ownership.js"
  },
  "keywords": [
    "godot",
    "codex-144-99",
    "sacred-geometry",
    "144:99-ratio",
    "lattice-system",
    "trauma-safe",
    "free",
    "open-source",
    "cathedral-of-circuits",
    "cathedral-network",
    "visionary"
  ],
  "author": "Rebecca Respawn <bekalah@users.noreply.github.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bekalah/cathedral.git",
    "directory": "packages/godot-codex-14499"
  },
  "homepage": "https://bekalah.github.io/cathedral/godot-codex-14499",
  "godot": {
    "version": "4.3+",
    "compatibility": "4.0+",
    "asset_type": "game_system",
    "themes": [
      "sacred_geometry",
      "mystical",
      "trauma_safe",
      "144_99_lattice"
    ]
  },
  "cathedral": {
    "sacred_mathematics": {
      "144_nodes": true,
      "99_gates": true,
      "144_99_ratio": 1.455,
      "golden_ratio": 1.618,
      "fibonacci_sequences": true
    },
    "trauma_safety": {
      "no_autoplay": true,
      "esc_exit": true,
      "motion_controls": true,
      "intensity_adjustment": true,
      "gentle_defaults": true
    },
    "integration": {
      "connects_to": [
        "godot-liber-arcanae",
        "godot-tesseract-bridge",
        "godot-vfx-library",
        "codex-144-99",
        "codex-144-99-core"
      ]
    },
    "design_philosophy": {
      "flowingInterfaces": true,
      "energyRibbons": true,
      "breathingAnimations": true,
      "sacredColors": true,
      "archetypalForms": true
    },
    "mode": "VISIONARY",
    "quality": "Gold Standard A+",
    "status": "Active"
  },
  "files": [
    "scenes/",
    "scripts/",
    "resources/",
    "materials/",
    "shaders/",
    "documentation/",
    "examples/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "vitest": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=8.15.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@cathedral/codex-144-99": "workspace:*",
    "@cathedral/codex-144-99-core": "workspace:*",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "eslint-config-prettier": "^9.1.0"
  },
  "consolidated": true,
  "consolidatedAt": "2025-12-04T22:13:01.328Z",
  "alchemical": {
    "name": "Godot Codex 14499",
