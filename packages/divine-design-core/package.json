  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@cathedral/sacred-geometry-core": "workspace:*",
    "@cathedral/sacred-mathematics-core": "workspace:*",
    "@cathedral/circuitum99-core": "workspace:*",
    "@cathedral/circuitum99-arcanae-cyoa": "workspace:*",
    "@cathedral/mystery-house-core": "workspace:*",
    "@cathedral/stone-grimoire": "workspace:*",
    "@cathedral/stone-grimoire-core": "workspace:*",
    "@cathedral/stone-grimoire-library-engine": "workspace:*",
    "@cathedral/liber-arcanae": "workspace:*",
    "@cathedral/liber-arcanae-core": "workspace:*",
    "@cathedral/liber-arcanae-tools": "workspace:*",
    "@cathedral/codex-144-99": "workspace:*",
    "codex-144-99-core": "workspace:*"
  },
  "keywords": [
    "alchemy",
    "hermeticism",
    "kabbalah",
    "sacred-geometry",
    "divine-design",
    "fusion-kink",
    "tree-of-life",
    "sephiroth",
    "cathedral-network",
    "art"
  ],
  "engines": {
    "node": "*",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.0.0",
  "cathedral": {
    "sacred_mathematics": {
      "sacredGeometry": true,
      "goldenRatio": 1.618,
      "cathedralRatio": 1.455,
      "fibonacciGrids": true,
      "divineProportions": true
