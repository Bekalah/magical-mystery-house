{
  "name": "@cathedral/godot-liber-arcanae",
  "version": "1.0.0",
  "description": "Godot Liber Arcanae - 78-card living tarot system for mystical games",
  "main": "liber_arcanae.gd",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "validate": "echo 'Validating tarot correspondences...' && node validate-tarot.js",
    "export": "echo 'Exporting tarot system...' && pnpm run build",
    "test": "echo 'Testing card interactions...' && node test-cards.js",
    "dev": "tsx watch src/index.ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build",
    "clean:all": "rimraf dist build node_modules/.turbo",
    "validate:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "benchmark": "vitest run --reporter=verbose --reporter=json --outputFile=benchmark-results.json",
    "validate:sacred-math": "node scripts/validate-sacred-math.js",
    "validate:trauma-safety": "node scripts/validate-trauma-safety.js",
    "validate:integration": "node scripts/validate-integration.js",
    "validate:ownership": "node scripts/validate-ownership.js"
  },
  "keywords": [
    "godot",
    "tarot",
    "liber-arcanae",
    "78-cards",
    "archetypes",
    "trauma-safe",
    "free",
    "open-source",
    "cathedral-of-circuits",
    "living-tarot"
  ],
  "author": "Rebecca Respawn <bekalah@users.noreply.github.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bekalah/cathedral.git",
    "directory": "packages/godot-liber-arcanae"
  },
  "homepage": "https://bekalah.github.io/cathedral/godot-liber-arcanae",
  "godot": {
    "version": "4.3+",
    "compatibility": "4.0+",
    "asset_type": "game_system",
    "themes": [
      "tarot",
      "archetypes",
      "trauma_safe",
      "living_systems"
    ]
  },
  "cathedral": {
    "sacred_mathematics": {
      "major_arcana": 22,
      "minor_arcana": 56,
      "total_cards": 78,
      "golden_ratio": 1.618,
      "divine_proportions": true
    },
    "trauma_safety": {
      "no_autoplay": true,
      "esc_exit": true,
      "motion_controls": true,
      "intensity_adjustment": true,
      "gentle_defaults": true,
      "consent_based": true
    },
    "living_system": {
      "archetype_beings": true,
      "interactive_cards": true,
      "fusion_kink_compatible": true,
      "trauma_healing": true
    },
    "integration": {
      "connects_to": [
        "godot-codex-14499",
        "godot-tesseract-bridge",
        "godot-vfx-library",
        "liber-arcanae",
        "liber-arcanae-core",
        "liber-arcanae-tools"
      ]
    },
    "design_philosophy": {
      "flowingInterfaces": true,
      "energyRibbons": true,
      "breathingAnimations": true,
      "sacredColors": true,
      "archetypalForms": true
    }
  },
  "files": [
    "scenes/",
    "scripts/",
    "resources/",
    "cards/",
    "archetypes/",
    "documentation/",
    "examples/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/node": "^20.10.0",
    "vitest": "^2.1.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0"
  },
  "types": "dist/index.d.ts",
  "packageManager": "pnpm@10.23.0",
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=8.15.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "dependencies": {
    "@cathedral/liber-arcanae": "workspace:*",
    "@cathedral/liber-arcanae-core": "workspace:*",
    "@cathedral/liber-arcanae-tools": "workspace:*",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "eslint-config-prettier": "^9.1.0"
  },
  "consolidated": true,
  "consolidatedAt": "2025-12-04T22:13:01.579Z",
  "alchemical": {
    "name": "Godot Liber Arcanae",
    "symbol": "⊙",
    "label": "⊙ Godot Liber Arcanae (@cathedral/godot-liber-arcanae)"
  }
}
