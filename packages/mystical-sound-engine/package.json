{
  "name": "@cathedral/mystical-sound-engine",
  "version": "1.0.0",
  "description": "Real-time audio synthesis engine for mystical research and meditation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "echo 'Tests for mystical sound synthesis'",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.{ts,tsx,js,jsx} --max-warnings=0",
    "lint:fix": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist build",
    "clean:all": "rimraf dist build node_modules/.turbo",
    "validate": "npm run type-check && npm run lint && npm run test",
    "validate:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "benchmark": "vitest run --reporter=verbose --reporter=json --outputFile=benchmark-results.json",
    "validate:sacred-math": "node scripts/validate-sacred-math.js",
    "validate:trauma-safety": "node scripts/validate-trauma-safety.js",
    "validate:integration": "node scripts/validate-integration.js",
    "validate:ownership": "node scripts/validate-ownership.js"
  },
  "keywords": [
    "cathedral",
    "mystical",
    "sound",
    "audio",
    "synthesis",
    "binaural-beats",
    "meditation"
  ],
  "author": "Cathedral Research Team",
  "license": "CC0-1.0",
  "dependencies": {
    "next": "^16.0.1",
    "typescript": "^5.3.0"
  },
  "cathedral": {
    "system": "audio",
    "sacred_ratio": "144:99",
    "frequencies": "solfeggio",
    "elements": [
      "fire",
      "water",
      "air",
      "earth"
    ],
    "sacred_mathematics": {
      "sacredGeometry": true,
      "goldenRatio": 1.618,
      "cathedralRatio": 1.455,
      "fibonacciGrids": true,
      "divineProportions": true
    },
    "trauma_safety": {
      "noAutoplay": true,
      "escExit": true,
      "motionControls": true,
      "intensityAdjustment": true,
      "gentleDefaults": true,
      "undoRedo": true
    },
    "design_philosophy": {
      "flowingInterfaces": true,
      "energyRibbons": true,
      "breathingAnimations": true,
      "sacredColors": true,
      "archetypalForms": true
    },
    "integration": {
      "connects_to": []
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Bekalah/cathedral.git",
    "directory": "packages/mystical-sound-engine"
  },
  "homepage": "https://github.com/Bekalah/cathedral/tree/main/packages/mystical-sound-engine",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "devDependencies": {
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "vitest": "^2.1.1",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint-config-prettier": "^9.1.0"
  },
  "consolidated": true,
  "consolidatedAt": "2025-12-01T19:36:57.744Z",
