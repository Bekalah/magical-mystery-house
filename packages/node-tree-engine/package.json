{
  "name": "@cathedral/node-tree-engine",
  "version": "1.0.0",
  "description": "Complete Node Tree Health System for 99 Mystical Codex Nodes with Health Monitoring, Interactive Maps, and Dynamic Stories",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "rollup -c -w",
    "test": "vitest",
    "lint": "eslint src/**/*.{js,ts}",
    "clean": "rm -rf dist",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint:fix": "eslint src/**/*.{ts,tsx,js,jsx} --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md,css,html}\"",
    "type-check": "tsc --noEmit",
    "clean:all": "rimraf dist build node_modules/.turbo",
    "validate": "npm run type-check && npm run lint && npm run test",
    "validate:ci": "npm run type-check && npm run lint && npm run test:coverage",
    "benchmark": "vitest run --reporter=verbose --reporter=json --outputFile=benchmark-results.json",
    "validate:sacred-math": "node scripts/validate-sacred-math.js",
    "validate:trauma-safety": "node scripts/validate-trauma-safety.js",
    "validate:integration": "node scripts/validate-integration.js",
    "validate:ownership": "node scripts/validate-ownership.js"
  },
  "dependencies": {
    "circus": "^3.0.0",
    "d3-hierarchy": "^3.1.2",
    "d3-force": "^3.0.0",
    "d3-selection": "^3.0.0",
    "graphql": "^16.8.1",
    "async-mutex": "^0.4.0",
    "fast-xml-parser": "^4.3.2",
    "marked": "^12.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "rollup": "^4.9.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/d3-hierarchy": "^3.1.6",
    "@types/d3-force": "^3.0.8",
    "@types/d3-selection": "^3.0.10",
    "@types/marked": "^5.0.2",
    "vitest": "^1.2.0",
    "typescript": "^5.6.2",
    "tsx": "^4.19.1",
    "eslint": "^9.11.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "@typescript-eslint/eslint-plugin": "^8.6.0",
    "@typescript-eslint/parser": "^8.6.0",
    "eslint-config-prettier": "^9.1.0"
  },
  "keywords": [
    "node-tree",
    "health-system",
    "mystical-tree",
    "codex-nodes",
    "interactive-map",
    "grimoire",
    "dynamic-stories"
  ],
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "license": "CC0-1.0",
  "consolidated": true,
  "consolidatedAt": "2025-12-01T19:36:57.819Z",
  "packageManager": "pnpm@10.23.0",
  "alchemical": {
    "name": "Node Tree",
    "symbol": "⊙",
    "label": "⊙ Node Tree (@cathedral/node-tree-engine)"
  },
  "cathedral": {
    "sacred_mathematics": {
      "sacredGeometry": true,
