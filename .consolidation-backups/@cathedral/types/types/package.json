{
  "name": "@cathedral/types",
  "version": "1.0.0",
  "description": "Shared TypeScript types for Cathedral monorepo",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "engines": {
    "node": ">=20.18.0",
    "pnpm": ">=8.15.0"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "author": "Rebecca Respawn <bekalah@users.noreply.github.com>",
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Bekalah/cathedral.git",
    "directory": "packages/types"
  },
  "homepage": "https://bekalah.github.io/cathedral/packages/types",
  "packageManager": "pnpm@8.15.0",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "alchemical": {
    "name": "Types",
    "symbol": "⊙",
    "label": "⊙ Types (@cathedral/types)"
  }
}
