  "devDependencies": {
    "@types/node": "^24.1.0",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "license": "CC0-1.0",
  "consolidated": true,
  "consolidatedAt": "2025-11-30T23:01:13.161Z",
  "packageManager": "pnpm@8.0.0",
  "alchemical": {
    "name": "Types",
    "symbol": "⊙",
