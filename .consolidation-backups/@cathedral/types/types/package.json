    "npm:publish": "node scripts/publish-npm.cjs",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "zod": "^3.25.61",
    "@cathedral/config-eslint": "workspace:^",
    "@cathedral/config-typescript": "workspace:^",
