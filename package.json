    "improve:all-day": "node scripts/keep-running.mjs",
    "improve:stop": "pkill -f keep-running.mjs || true",
    "improve:status": "ps aux | grep -E 'keep-running|10-hour-improvement' | grep -v grep || echo 'Not running'",
    "improve:watch": "node scripts/monitor-cycles.mjs --watch 30",
    "improve:check": "node scripts/monitor-cycles.mjs",
    "fixes:watch": "node scripts/live-fixes-display.mjs",
    "health": "node scripts/monorepo-health-monitor.mjs",
    "health:watch": "node scripts/health-dashboard.mjs",
    "health:dashboard": "node scripts/health-dashboard.mjs",
    "scope": "node scripts/scope-analyzer.mjs",
    "monitor": "node scripts/health-dashboard.mjs",
    "health:permanent": "node scripts/permanent-health-service.mjs",
    "cleanup:verify": "node scripts/cleanup-flat-files.mjs --dry-run",
    "system:map": "node scripts/system-connector.mjs",
    "system:connect": "node scripts/system-connector.mjs",
    "fusionkink:integrate": "node scripts/fusionkink-integrator.mjs",
    "fusionkink:scan": "node scripts/fusionkink-integrator.mjs",
    "fix:all": "node scripts/comprehensive-error-fixer.mjs",
    "fix:errors": "node scripts/comprehensive-error-fixer.mjs",
    "update:versions": "node scripts/update-version-pinning.mjs",
    "label:all": "node scripts/system-labeler.mjs && node tools/apply-alchemical-labels.mjs",
    "label:systems": "node scripts/system-labeler.mjs",
    "deduplicate": "node scripts/real-deduplicator.mjs",
    "deduplicate:all": "node scripts/real-deduplicator.mjs",
    "deduplicate:old": "node scripts/comprehensive-deduplicator.mjs",
    "publish:connect": "node scripts/publisher-connector.mjs",
    "publish:prepare": "node scripts/publisher-connector.mjs",
    "unify": "node scripts/unified-system-builder.mjs",
    "unify:all": "node scripts/unified-system-builder.mjs",
    "publish:master": "node scripts/master-publisher.mjs",
    "publish:all": "node scripts/master-publisher.mjs",
    "fix:real": "node scripts/real-fixer.mjs",
    "fix:duplicates": "node scripts/real-fixer.mjs",
    "quality:analyze": "node scripts/quality-analyzer.mjs",
    "quality:learn": "node scripts/quality-analyzer.mjs",
    "registry:build": "node scripts/create-comprehensive-registry.mjs",
    "discover:all": "node tools/comprehensive-discovery.mjs",
    "analyze:partials": "node tools/partial-analyzer.mjs",
    "analyze:codex": "node tools/codex-alignment-analyzer.mjs",
    "sync:remotes": "node tools/include-remote-repos.mjs",
    "discover:full": "pnpm run sync:remotes && node tools/comprehensive-discovery.mjs && node tools/partial-analyzer.mjs && node tools/codex-alignment-analyzer.mjs",
    "consolidate:all": "node tools/consolidate-all-workspaces.mjs",
    "consolidate": "node tools/comprehensive-consolidator.mjs",
    "test:commands": "node scripts/test-all-commands.mjs",
    "fix:all-issues": "node tools/fix-all-issues.mjs",
    "audit:all": "node tools/comprehensive-audit-system.mjs",
    "audit:complete": "node tools/comprehensive-audit-system.mjs",
    "fix:licensing": "node tools/fix-licensing.mjs",
    "complete:help": "node tools/completion-helper.mjs",
    "organize:all": "node tools/organize-and-merge-all.mjs",
    "debug:all": "node tools/debug-all.mjs",
    "push:github": "node tools/push-to-github.mjs",
    "organize:complete": "node tools/organize-debug-merge-push.mjs",
    "auto:fix": "node tools/auto-fix-all.mjs",
    "auto:fix:all": "node tools/auto-fix-all.mjs",
    "connect:all": "node tools/connect-and-fix-all.mjs",
    "connect:fix": "node tools/connect-and-fix-all.mjs",
    "fix:deployments": "node tools/fix-all-deployments.mjs",
    "deploy:fix": "node tools/fix-all-deployments.mjs",
    "cleanup:complete": "node tools/complete-cleanup.mjs",
    "cleanup:all": "node tools/complete-cleanup.mjs",
    "cleanup:space": "node tools/cleanup-space.mjs",
    "connect:remotes": "node tools/connect-remotes.mjs",
    "review:repos": "node tools/review-all-repos.mjs",
    "connect:cathedral": "node tools/connect-cathedral-work.mjs",
    "update:codex": "node tools/update-codex-circuitum99.mjs",
    "consolidate:master": "node tools/consolidate-to-master.mjs",
    "theme:apply": "node tools/apply-unified-theme.mjs",
    "theme:unified": "node tools/apply-unified-theme.mjs",
    "label:alchemical": "node tools/apply-alchemical-labels.mjs",
    "consolidate:workspaces": "node tools/consolidate-all-workspaces.mjs",
    "consolidate:execute": "node tools/execute-consolidation.mjs",
    "consolidate:merge": "node tools/execute-consolidation.mjs",
    "registry:update": "node scripts/create-comprehensive-registry.mjs",
    "publish:packages": "pnpm -r publish --access public",
    "deploy:vercel": "vercel --prod",
    "deploy:docs": "gh-pages -d docs"
  },
  "devDependencies": {
    "turbo": "^1.10.0",
    "typescript": "^5.0.0",
    "glob": "^10.3.10",
    "@types/node": "^20.0.0"
  },
  "engines": {
    "node": "*",
    "pnpm": "*"
  },
  "license": "CC0-1.0",
  "packageManager": "pnpm@10.23.0",
  "alchemical": {
    "name": "Cathedral Master Deployment",
    "symbol": "⊙",
    "label": "⊙ Cathedral Master Deployment (cathedral-master-deployment)"
  }
}
