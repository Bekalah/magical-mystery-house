{
  "name": "cathedral-master-deployment",
  "version": "1.0.0",
  "private": true,
  "description": "Cathedral Master Deployment - Monorepo",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "type-check": "turbo type-check",
    "test": "turbo test",
    "clean": "turbo clean",
    "save": "node scripts/easy-save-system.mjs",
    "backup": "node scripts/comprehensive-backup-system.mjs",
    "backup:all": "node scripts/comprehensive-backup-system.mjs && node scripts/easy-save-system.mjs",
    "cleanup": "node scripts/cleanup-flat-files.mjs",
    "security:audit": "node scripts/security-audit.mjs",
    "security:check": "pnpm audit && pnpm run security:audit",
    "security:fix": "pnpm audit --fix",
    "maintain": "pnpm run cleanup && pnpm run security:audit && pnpm build",
    "improve": "node scripts/run-improvement-experiment.mjs",
    "improve:tonight": "node scripts/run-experiment-tonight.mjs",
