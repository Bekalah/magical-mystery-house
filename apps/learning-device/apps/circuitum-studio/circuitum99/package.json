    "tone": "^14.7.77",
    "@cathedral/sacred-geometry-core": "workspace:*",
    "@cathedral/mystical-data-unified": "workspace:*",
    "@cathedral/gem-tower-engine": "workspace:*",
    "@cathedral/fusion-kink-generator": "workspace:*",
    "@cathedral/smooth-movement-system": "workspace:*",
    "@cathedral/portal-system": "workspace:*"
